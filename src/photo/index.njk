---
layout: index.njk
title: photo
override:tags: []
---
<article class="photo list">
  <h1>pho<br/>to</h1>
  <section class="content">
  {%- for photo in collections.photo | reverse %}
    <article class="photo">
      <div class="button">
        <a class="content" href="{{ photo.page.url }}">
          {#- sizes generated by https://github.com/peter-neumann-dev/responsive-image-linter #}
          {% image photo.data.responsive, photo.data.alt, "(min-width: 2800px) calc(11vw - 70px), (min-width: 2520px) calc(12.69vw - 83px), (min-width: 2240px) calc(14.23vw - 83px), (min-width: 1980px) calc(16.67vw - 92px), (min-width: 1700px) calc(20vw - 103px), (min-width: 1440px) calc(25vw - 120px), (min-width: 1160px) calc(33.08vw - 145px), (min-width: 880px) calc(50vw - 204px), (min-width: 640px) calc(100vw - 372px), calc(3.75vw + 225px)" %}
          <h3>{{ photo.data.title }}</h3>
          <span class="number">{{ photo.data.number | shortNumber }}</span>
          <span class="date">{{ photo.page.date | shortDate }}</span>
        </a>
      </div>
    </article>
  {%- endfor %}
  </section>
</article>